@model IEnumerable<MainAuth.Models.Doctor>

@{
    ViewData["Title"] = "Index";
}

<h1>Doctors List</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Dname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Special)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Dname)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Special)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.DoctorId">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.DoctorId">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.DoctorId">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
<br />
<br />
<a asp-controller="DocPat" asp-action="Patient">Patients List</a>
